//                                Declarations
//===----------------------------------------------------------------------===//

@_moveOnly
class Klass {
  deinit
}

@_moveOnly
struct TrivialStruct {
  var i: Builtin.Int32
}

@_moveOnly
struct SingleFieldNonTrivial {
  var k: Klass
}

@_moveOnly
struct ThreeNonTrivial {
  var k: Klass
  var i: Builtin.Int32
  var k2: Klass
}

@_moveOnly
struct ThreeNonTrivialNoDeinit {
  var k: Klass
  var i: Builtin.Int32
  var k2: Klass
}

